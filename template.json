{
  "type": "container",
  "title": "GLPI",
  "description": "Free IT and Asset Management Software",
  "logo": "https://raw.githubusercontent.com/glpi-project/glpi/master/pics/logos/logo-GLPI-250-black.png",
  "image": "cmotta2016/glpi:latest",
  "restart_policy": "unless-stopped"
  "ports": [
    "80/tcp"    
  ]
  "volumes": [
     {
       "container": "/var/www/glpi",
       "bind": "/opt/systems/glpi"
     }
  ]
}
